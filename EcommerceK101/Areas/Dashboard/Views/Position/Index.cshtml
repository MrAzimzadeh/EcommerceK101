@model IEnumerable<Position>
<div class="container-fluid">
    <!-- DataTales Example -->
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">
                <a href="/dashboard/Position/create">Create</a>
            </h6>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                    <thead>
                        <tr>
                            <th>#Id</th>
                            <th>Name</th>
                            <th> Count </th>
                            <th>
                                Parametr
                            </th>
                        </tr>
                    </thead>
                    <tfoot>
                        <tr>
                            <th>#Id</th>
                            <th>Name</th>
                            <th>Count</th>
                            <th>
                                Parametr
                            </th>
                        </tr>
                    </tfoot>
                    <tbody>
                        @* *@
                        @foreach (var position  in Model)
                        {
                            <tr>
                                <th>@position.Id</th>
                                <th>
                                    @position.PositionName
                                </th>
                                <th>
                                    
                                </th>

                                <th class="d-flex">
                                    <a href="/dashboard/position/Detail/@position.Id" class="btn btn-secondary">
                                        Deteil
                                    </a>
                                    <a href="/dashboard/position/Delete/@position.Id" class="btn btn-danger w-50">
                                        Delete
                                    </a>
                                    <a href="/dashboard/position/Edit/@position.Id" class="btn btn-primary w-50">Update</a>
                                </th>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>